{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths" : {
      "@enviroments/*": [
        "src/environments/*"
      ],
      "@app/*": [
        "src/app/*"
      ],
      "@app/assets/*": [
        "src/assets/*"
      ],
      "@services/*" : [
        "src/app/modules/dinamika/services/*"
      ],
      "@models/*" : [
        "src/app/modules/dinamika/models/*"
      ],
      "@components/*" : [
        "src/app/modules/dinamika/components/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
